    <Paragraph Type="Character">
      <Text>CHRIS</Text>
    </Paragraph>
    <Paragraph Type="Dialogue">
      <Text>But...?</Text>
    </Paragraph>
    <Paragraph Type="Character">
      <Text>LANCE</Text>
    </Paragraph>
    <Paragraph Type="Dialogue">
      <Text>I don't know.</Text>
